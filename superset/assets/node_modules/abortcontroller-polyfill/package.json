    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^3.0.3",
    "wdio-jasmine-framework": "^0.3.7",
    "wdio-selenium-standalone-service": "0.0.11",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.0"
  }


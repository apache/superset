  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^27.3.1",
    "prettier": "^2.8.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",

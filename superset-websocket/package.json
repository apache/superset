    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",

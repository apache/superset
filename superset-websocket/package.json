  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.3",

    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^2.0.0",
    "typescript": "~5.8.3",
    "webpack": "^5.99.9"
  },
  "browserslist": {
    "production": [
      ">0.5%",

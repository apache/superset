{
  "id": "extension1",
  "name": "Extension 1",
  "version": "1.0.0",
  "frontend": {
    "contributions": {
      "commands": [
        {
          "command": "extension1.copy_query",
          "icon": "CopyOutlined",
          "title": "Copy Query",
          "description": "Copy the current query to clipboard"
        },
        {
          "command": "extension1.prettify",
          "title": "Prettify",
          "description": "Prettify the current query"
        },
        {
          "command": "extension1.clear",
          "title": "Clear",
          "description": "Clear the current query"
        },
        {
          "command": "extension1.refresh",
          "title": "Refresh",
          "description": "Refresh the current panel"
        }
      ],
      "views": {
        "sqllab.panels": [
          {
            "id": "extension1.component",
            "name": "Primitives"
          }
        ]
      },
      "menus": {
        "sqllab.editor": {
          "primary": [
            {
              "view": "builtin.editor",
              "command": "extension1.copy_query"
            }
          ],
          "secondary": [
            {
              "view": "builtin.editor",
              "command": "extension1.prettify"
            }
          ],
          "context": [
            {
              "view": "builtin.editor",
              "command": "extension1.clear"
            },
            {
              "view": "builtin.editor",
              "command": "extension1.refresh"
            }
          ]
        },
        "sqllab.panel": {
          "context": [
            {
              "view": "extension1.component",
              "command": "extension1.refresh"
            }
          ]
        }
      }
    },
    "moduleFederation": {
      "exposes": ["./index"]
    }
  },
  "backend": {
    "entryPoints": ["extension1.entrypoint"],
    "files": ["backend/src/extension1/**/*.py"]
  },
  "dependencies": [],
  "permissions": [
    "can_read",
    "can_view",
    "can_copy_query",
    "can_prettify",
    "can_clear",
    "can_refresh"
  ]
}

{
  "id": "sqllab_parquet",
  "name": "Export SQL Lab to Parquet",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "frontend": {
    "contributions": {
      "commands": [
        {
          "command": "sqllab_parquet.export",
          "title": "Export to Parquet"
        }
      ],
      "menus": {
        "sqllab.editor": {
          "secondary": [
            {
              "view": "builtin.editor",
              "command": "sqllab_parquet.export"
            }
          ]
        }
      }
    },
    "moduleFederation": {
      "exposes": ["./index"]
    }
  },
  "backend": {
    "entryPoints": ["sqllab_parquet.entrypoint"],
    "files": ["backend/src/sqllab_parquet/**/*.py"]
  },
  "permissions": ["can_read", "can_view"]
}

{
  "lerna": "3.2.1",
  "npmClient": "yarn",
  "packages": [
    "packages/*",
    "plugins/*"
  ],
  "useWorkspaces": true,
  "version": "0.13.9",
  "ignoreChanges": [
    "**/*.md",
    "**/*.spec.tsx?",
    "**/*.spec.jsx?",
    "**/__mocks__/**"
  ],
  "command": {
    "publish": {
      "message": "chore(release): publish %s",
      "graphType": "all"
    },
    "version": {
      "message": "chore(release): publish %s",
      "exact": true
    }
  }
}

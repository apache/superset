{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "declarationDir": "lib",
    "outDir": "lib",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "@superset-ui/core": ["src"],
      "@superset-ui/core/*": ["src/*"]

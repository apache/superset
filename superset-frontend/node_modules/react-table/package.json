{
  "name": "react-table",
  "version": "7.0.4",
  "description": "Hooks for building lightweight, fast and extendable datagrids for React",
  "license": "MIT",
  "homepage": "https://github.com/tannerlinsley/react-table#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerlinsley/react-table.git"
  },
  "keywords": [
    "react",
    "table",
    "react-table",
    "datagrid"
  ],
  "main": "index.js",
  "scripts": {
    "test": "is-ci \"test:ci\" \"test:dev\"",
    "test:dev": "jest --watch",
    "test:ci": "yarn test:jest",
    "test:jest": "jest --coverage",
    "test:coverage": "yarn test:jest; open coverage/lcov-report/index.html",
    "build": "cross-env NODE_ENV=production rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn build",
    "release": "yarn publish",
    "releaseNext": "yarn publish --tag next",
    "format": "prettier {src,src/**,examples/*/src,examples/*/src/**}/*.{md,js,jsx,tsx} --write",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "docz:serve": "docz build && docz serve",
    "postinstall": "node ./scripts/postinstall.js || exit 0"
  },
  "files": [
    "CHANGELOG.md",
    "src/**/*.js",
    "index.d.ts",
    "dist",
    "README.md",
    "scripts/"
  ],
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@scarf/scarf": "^0.1.5"
  },
